<div class="container text-center">
  <div class="container_recap">
    <br>
    <h1 style="color: #60919F; font-size: 22px">RECAPITULATIF DE VOTRE PROCEDURE</h1>
    <div class="recap">

      <div class="progress-wrap progress" data-progress-percent="<%= (12 - @champ_not_done)*100/12 %>">
        <div class="progress-bar progress"></div>
      </div>
      <h1><%= @procedure.full_name %></h1>
      <div class="metrics">
        <div class="metrics-question">
          <i class="fa fa-question" style="font-size: 20px; margin: 5px" aria-hidden="true"></i>
          <p><%= @champ_not_done %> champs à remplir</p>
        </div>

        <div class="metrics-document">
          <i class="fa fa-file-text-o" style="font-size: 20px ; margin: 5px" aria-hidden="true"></i>
          <p><%= @doc_not_upload %> documents à uploader</p>
        </div>
      </div>

      <div class="button-questionnaire">
          <%= link_to "RETOUR AU QUESTIONNAIRE", edit_procedure_path, style: "color: #60919F" %>
      </div>
      <%= link_to "TROUVER MON AVOCAT", avocats_path, class: "btn btn-call-to-action" %>


      <div class="wrapper-progress ">
        <ul class="tabs clearfix" data-tabgroup="first-tab-group">
          <li><a href="#tab1" class="active">Situation familiale</a></li>
          <li><a href="#tab2">Situation financière</a></li>
          <li><a href="#tab3">Situation foncière</a></li>
        </ul>
        <section id="first-tab-group" class="tabgroup">
          <div id="tab1">
            <p>Enfants: <%= @procedure.child_nb %></p>
            <p>Type de Contrat de Mariage: <%= @procedure.contract_type %></p>
          </div>
          <div id="tab2">
            <div id="bis text-left">
              <p>Votre Siuation Professionnelle: <%= @procedure.status_pro %></p>
              <p>La Siuation Professionnelle de Votre Conjoint: <%= @procedure.status_pro_conjoint %></p>
              </div>
            <div id="bis text-left">
              <p>Relevé bancaire: <%= @procedure.bank_account_nb %></p>
              <p>Crédit bancaire: <%= @procedure.credit_nb %></p>

            </div>
          </div>
          <div id="tab3">
            <div id="bis text-left">
              <p>Logements dont vous êtes propriétaire: <%= @procedure.owner_nb %></p>
              <p>Logements dont vous êtes locataire: <%= @procedure.renter_nb %></p>
            </div>
            <div id="ter text-left">
              <p>Véhicule: <%= @procedure.vehicle_nb %></p>
              <p>Assurance: <%= @procedure.insurance_nb %></p>
            </div>

          </div>
        </section>
      </div>
    </div>
  </div>


