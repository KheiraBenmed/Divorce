                <div class="doc-description"><ol>Quittance de loyer</ol></div>
                <% if @procedure[:rent].nil? %>
                  <div class="doc-category btn-call-to-action text-center">SCANNER OU TELECHARGER</div>
                  <div class="badge badge-red"><i class="fa fa-times" aria-hidden="true"></i></div>
                <% end %>
            </div>
          </div>
        <% end %>
      <% end %>

      <% if !@procedure.child_nb.nil? %>
        <% if @procedure.child_nb > 0 %>
          <div class="col-xs-12 col-sm-6 col-lg-4">
            <div class="doc">
              <div class="doc-description"><ol>Certificats de scolarité</ol></div>
              <% if @procedure[:scolarite].nil? %>
                <div class="doc-category btn-call-to-action text-center">SCANNER OU TELECHARGER</div>
                <div class="badge badge-red"><i class="fa fa-times" aria-hidden="true"></i></div>
              <% end %>
            </div>
          </div>
        <% end %>
      <% end %>

      <% if !@procedure.child_nb.nil? %>
        <% if @procedure.child_nb > 0 %>
          <div class="col-xs-12 col-sm-6 col-lg-4">
            <div class="doc">
              <div class="doc-description"><ol>Relevé de la CAF</ol></div>
              <% if @procedure[:caf].nil? %>
                <div class="doc-category btn-call-to-action text-center">SCANNER OU TELECHARGER</div>
                <div class="badge badge-red"><i class="fa fa-times" aria-hidden="true"></i></div>
              <% end %>
            </div>
          </div>
        <% end %>
      <% end %>

      <% if !@procedure.vehicle_nb.nil? %>
        <% if @procedure.vehicle_nb > 0 %>
          <div class="col-xs-12 col-sm-6 col-lg-4">
            <div class="doc">
              <div class="doc-description"><ol>Carte grise de vos véhicules</ol></div>
              <% if @procedure[:carte_grise].nil? %>
                <div class="doc-category btn-call-to-action text-center">SCANNER OU TELECHARGER</div>
                <div class="badge badge-red"><i class="fa fa-times" aria-hidden="true"></i></div>
              <% end %>
            </div>
          </div>
        <% end %>
      <% end %>

      <% if !@procedure.vehicle_nb.nil? %>
        <% if @procedure.vehicle_nb > 0 %>
          <div class="col-xs-12 col-sm-6 col-lg-4">
            <div class="doc">
              <div class="doc-description"><ol>Assurance véhicule</ol></div>
              <% if @procedure[:insurance_vehicle].nil? %>
                <div class="doc-category btn-call-to-action text-center">SCANNER OU TELECHARGER</div>
                <div class="badge badge-red"><i class="fa fa-times" aria-hidden="true"></i></div>
              <% end %>
            </div>
          </div>
        <% end %>
      <% end %>


      <% if !@procedure.renter_nb.nil? && !@procedure.owner_nb.nil? %>
        <% if @procedure.renter_nb > 0 && @procedure.owner_nb > 0 %>
          <div class="col-xs-12 col-sm-6 col-lg-4">
            <div class="doc">
              <div class="doc-description"><ol>Justificatif de domicile - Facture EDF ou telephone</ol></div>
              <% if @procedure[:bills].nil? %>
                <div class="doc-category btn-call-to-action text-center">SCANNER OU TELECHARGER</div>
                <div class="badge badge-red"><i class="fa fa-times" aria-hidden="true"></i></div>
              <% end %>
            </div>
          </div>
        <% end %>
      <% end %>

      <% if !@procedure.insurance_nb.nil? %>
        <% if @procedure.insurance_nb > 0 %>
          <div class="col-xs-12 col-sm-6 col-lg-4">
            <div class="doc">
              <div class="doc-description"><ol>Justificatifs d'Assurance</ol></div>
              <% if @procedure[:insurance_other].nil? %>
                <div class="doc-category btn-call-to-action text-center">SCANNER OU TELECHARGER</div>
                <div class="badge badge-red"><i class="fa fa-times" aria-hidden="true"></i></div>
              <% end %>
            </div>
          </div>
        <% end %>
      <% end %>

      </ul>
    </div>
  </div>
</div>

<!-- Modal for camera -->
<div class="modal fade" id="camMod" tabindex="-1" role="dialog" aria-labelledby="camMLb">
  <div class="modal-dialog modal-lg" style="width: 670px; padding: 0px" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">×</span>
