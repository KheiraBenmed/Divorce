        <video id="video" width="100%" height="480" autoplay></video>
        <canvas id="canvas" width="100%" height="480" class="hide"></canvas>
        <div class="text-center">
          <a class="btn btn-primary" style="margin: 10px" id="snap" data-type='1'>Snap Photo</a>
        </div>
      </div>
    </div>
  </div>
</div>


