 <% @questions.each_with_index do | question, index | %>
    <% answer = @procedure.send(question['column']) %>
    <% if answer.is_a? DocumentUploader %>
      <% answer = @procedure.send(question['column'] + "_url") %>
    <% end %>

    <% if answer.nil? %>
      <i class="fa progress-point fa-circle-thin" data-number="<%= index + 1 %>" aria-hidden="true"></i>
    <% else answer.blank? && ![ true, false ].include?(answer) %>
      <i class="fa progress-point fa-check-circle" data-number="<%= index + 1 %>" aria-hidden="true"></i>
    <% end %>
  <% end %>

